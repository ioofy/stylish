{
  "extends": "../../.eslintrc.js",
  "overrides": [
    {
      "files": ["rollup.config.js"],
      "parserOptions": {
        "version": 2018,
        "sourceType": "module",
        "ecmaFeatures": {
          "modules": true
        }
      }
    }
  ],
  "rules": {
    "import/no-extraneous-dependencies": ["off"],
    "import/prefer-default-export": "off",
    "@typescript-eslint/no-var-requires": ["off"],
    "react/self-closing-comp": ["off"]
  }
}
